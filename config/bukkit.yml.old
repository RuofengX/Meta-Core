settings:
  allow-end: true
  warn-on-overload: false
  permissions-file: permissions.yml
  update-folder: update
  plugin-profiling: false
  connection-throttle: -1
  query-plugins: true
  deprecated-verbose: default
  shutdown-message: OAR-01 closed
  minimum-api: none
  world-container: /mnt/oar-01-worlds/worlds
spawn-limits:
  monsters: 40
  animals: 15
  water-animals: 10
  water-ambient: 10
  ambient: 10
chunk-gc:
  period-in-ticks: 1200
ticks-per:
  animal-spawns: 400
  monster-spawns: 10
  water-spawns: 400
  water-ambient-spawns: 400
  ambient-spawns: 400
  autosave: 0
aliases: now-in-commands.yml
worlds:
  earth:
    generator: Terra:DEFAULT
  earth_nether:
    generator: Terra:NETHER